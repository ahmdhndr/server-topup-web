<table id="example2" class="table table-bordered table-hover w-100">
  <thead>
    <tr>
      <th>No</th>
      <th>Nama Koin</th>
      <th>Jumlah Koin</th>
      <th>Harga Koin</th>
      <th>Aksi</th>
    </tr>
  </thead>
  <tbody>
    <% for (let i=0; i < nominal.length; i++) { %>
      <tr>
        <td class="align-middle">
          <%= i + 1 %>
        </td>
        <td class="align-middle">
          <%= nominal[i].coinName %>
        </td>
        <td class="align-middle">
          <%= nominal[i].coinQty %>
        </td>
        <td class="align-middle">
          <%= nominal[i].price %>
        </td>
        <td>
          <form action="/nominal/delete/<%= nominal[i]._id %>?_method=DELETE" method="POST">
            <a href="/nominal/edit/<%= nominal[i]._id %>" class="btn btn-warning mr-1">
              <i class="fas fa-pencil-alt mr-1"></i>
              Ubah
            </a>
            <button type="submit" class="btn btn-danger">
              <i class="fas fa-trash mr-1"></i>
              Hapus
            </button>
          </form>
        </td>
      </tr>
      <% } %>
  </tbody>
</table>