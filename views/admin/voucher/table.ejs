<table id="example2" class="table table-bordered table-hover w-100">
  <thead>
    <tr>
      <th>No</th>
      <th>Nama Game</th>
      <th>Kategori</th>
      <th>Nominal</th>
      <th>Status</th>
      <th>Aksi</th>
    </tr>
  </thead>
  <tbody>
    <% for (let i=0; i < voucher.length; i++) { %>
      <tr>
        <td class="align-middle">
          <%= i + 1 %>
        </td>
        <td class="align-middle">
          <%= voucher[i].name %>
        </td>
        <td class="align-middle">
          <%= voucher[i]?.category?.name ?? '-' %>
        </td>
        <td class="align-middle">
          <% voucher[i].nominals.forEach((nominal)=> { %>
            <span class="badge badge-warning">
              <%= nominal.coinName %>
                <%= nominal.coinQty %>
            </span>
            <% }) %>
        </td>
        <td class="align-middle">
          <% if (voucher[i].status==="Y" ) { %>
            <span class="badge badge-success">Aktif</span>
            <% } else { %>
              <span class="badge badge-danger">Tidak Aktif</span>
              <% } %>
        </td>
        <td class="align-middle">
          <form action="/voucher/status/<%= voucher[i]._id %>?_method=PUT" method="POST" class="d-inline-block">
            <button type="submit" class="btn btn-sm btn-primary" data-toggle="tooltip" data-placement="top"
              title="Ubah Status">
              <i class=" fas fa-sync"></i>
            </button>
          </form>
          <form action="/voucher/delete/<%= voucher[i]._id %>?_method=DELETE" method="POST" class="d-inline-block">
            <a href="/voucher/edit/<%= voucher[i]._id %>" class="btn btn-sm btn-info" data-toggle="tooltip"
              data-placement="top" title="Edit Voucher">
              <i class="fas fa-pencil-alt"></i>
            </a>
            <button type="submit" class="btn btn-sm btn-danger" data-toggle="tooltip" data-placement="top"
              title="Hapus Voucher">
              <i class="fas fa-trash"></i>
            </button>
          </form>
        </td>
      </tr>
      <% } %>
  </tbody>
</table>