<% if (alert.message !='' ) { %>
  <div class="show-alert alert alert-<%= alert.status %> alert-dismissible">
    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
    <i class="icon <%= alert.status == 'success' ? 'fas fa-check' : 'fas fa-times' %>"></i> <%= alert.message %>
  </div>
<% } %>